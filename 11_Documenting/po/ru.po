#: src/guesser.c:172
#, c-format
msgid "Number %d is too big to convert it to Roman\n"
msgstr "Число %d слишком велико для перевода в римскую систему счисления\n"

#: src/guesser.c:195
#, c-format
msgid "Can't convert character %c from Roman to decimal number\n"
msgstr ""
"Невозможно перевести букву %c из римской системы счисления в десятичную\n"

#: src/guesser.c:231
msgid "Usage:"
msgstr "Использование:"

#: src/guesser.c:232
msgid " [OPTION...]"
msgstr " [ПАРАМЕТР...]"

#: src/guesser.c:233
msgid "Give a short usage message"
msgstr "Вывести короткую подсказку"

#: src/guesser.c:234
msgid "Give this help list"
msgstr "Вывести эту подсказку"

#: src/guesser.c:236
msgid "Use roman numbers"
msgstr "Использовать римские цифры"

#: src/guesser.c:237
msgid "Numbers guesser.\vSupported languages: English, Russian."
msgstr "Угадыватель чисел.\vПоддерживаемые языки: английский, русский."

#: src/guesser.c:249
msgid "Yes"
msgstr "Да"

#: src/guesser.c:250
msgid "No"
msgstr "Нет"

#: src/guesser.c:254
#, c-format
msgid "Guess a number between %s and %s.\n"
msgstr "Загадайте число между %s и %s.\n"

#: src/guesser.c:256
#, c-format
msgid "Guess a number between %d and %d.\n"
msgstr "Загадайте число между %d и %d.\n"

#: src/guesser.c:263
#, c-format
msgid "Is the guessed number greater than %s? (%s/%s) "
msgstr "Загаданное число больше %s? (%s/%s) "

#: src/guesser.c:265
#, c-format
msgid "Is the guessed number greater than %d? (%s/%s) "
msgstr "Загаданное число больше %d? (%s/%s) "

#: src/guesser.c:268
#, c-format
msgid "Got the end of input or an error, aborting\n"
msgstr "Получен конец ввода или ошибка, выключение\n"

#: src/guesser.c:283
#, c-format
msgid "Please enter just \"%s\" or just \"%s\"\n"
msgstr "Пожалуйста, введите просто \"%s\" или просто \"%s\"\n"

#: src/guesser.c:289
#, c-format
msgid "You guessed %s!\n"
msgstr "Вы загадали %s!\n"

#: src/guesser.c:291
#, c-format
msgid "You guessed %d!\n"
msgstr "Вы загадали %d!\n"
